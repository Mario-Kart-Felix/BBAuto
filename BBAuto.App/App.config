<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="BBAuto" connectionString="Data Source=localhost;Initial Catalog=BBAuto;Integrated Security=True;Connection Timeout=0" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BBAutoServiceSoap" maxBufferSize="64000000" maxReceivedMessageSize="64000000" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:56202/BBAutoService.asmx" binding="basicHttpBinding"
                bindingConfiguration="BBAutoServiceSoap" contract="BBAutoWebService.BBAutoServiceSoap"
                name="BBAutoServiceSoap" />
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>
